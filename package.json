{
  "name": "propertify",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "npx --legacy-peer-deps @cloudflare/next-on-pages",
    "preview": "npm run pages:build && wrangler pages dev",
    "deploy": "npm run pages:build && wrangler pages deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "db:init": "wrangler d1 execute prod-propertify --command \"SELECT 1;\" --local --yes > /dev/null 2>&1",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "db:studio:local": "npm run db:init && CLOUDFLARE_D1_LOCAL_URL=$(find ./.wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit) npm run db:studio",
    "db:push:local": "npm run db:init && CLOUDFLARE_D1_LOCAL_URL=$(find ./.wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit) npm run db:push",
    "db:migrate:local": "npm run db:init && CLOUDFLARE_D1_LOCAL_URL=$(find ./.wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit) npm run db:migrate",
    "format": "prettier --check --ignore-path .gitignore .",
    "format:fix": "prettier --write --ignore-path .gitignore ."
  },
  "dependencies": {
    "@clerk/elements": "0.22.5",
    "@clerk/nextjs": "6.9.6",
    "@hookform/resolvers": "3.9.1",
    "@radix-ui/react-alert-dialog": "^1.1.4",
    "@radix-ui/react-avatar": "1.1.2",
    "@radix-ui/react-dialog": "1.1.4",
    "@radix-ui/react-dropdown-menu": "2.1.4",
    "@radix-ui/react-label": "2.1.1",
    "@radix-ui/react-separator": "1.1.1",
    "@radix-ui/react-slot": "1.1.1",
    "@radix-ui/react-tooltip": "1.1.6",
    "@tanstack/react-form": "^0.40.4",
    "@tanstack/react-query": "5.62.8",
    "@tanstack/react-query-devtools": "5.62.8",
    "@tanstack/react-table": "^8.20.6",
    "@tanstack/react-virtual": "^3.11.2",
    "@tanstack/zod-form-adapter": "^0.40.4",
    "@trpc/client": "11.0.0-rc.630",
    "@trpc/react-query": "11.0.0-rc.630",
    "@trpc/server": "11.0.0-rc.638",
    "bunshi": "2.1.5",
    "class-variance-authority": "0.7.1",
    "client-only": "^0.0.1",
    "cloudflare-workers-cache": "^0.0.6",
    "clsx": "2.1.1",
    "cmdk": "^1.0.4",
    "date-fns": "^4.1.0",
    "drizzle-orm": "0.38.2",
    "drizzle-zod": "0.6.0",
    "framer-motion": "12.0.0-alpha.1",
    "immer": "10.1.1",
    "jotai": "2.10.4",
    "jotai-effect": "^1.0.7",
    "jotai-immer": "0.4.1",
    "jotai-tanstack-query": "0.9.0",
    "lucide-react": "0.469.0",
    "next": "15.1.2",
    "next-themes": "0.4.4",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "7.54.2",
    "react-hook-form-persist": "^3.0.0",
    "react-resizable-panels": "2.1.7",
    "server-only": "^0.0.1",
    "sonner": "1.7.1",
    "svix": "1.44.0",
    "tailwind-merge": "2.5.5",
    "tailwindcss-animate": "1.0.7",
    "uuid": "11.0.3",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "1.13.7",
    "@cloudflare/workers-types": "4.20241218.0",
    "@tanstack/eslint-plugin-query": "5.62.1",
    "@types/node": "22.10.2",
    "@types/react": "19.0.2",
    "@types/react-dom": "19.0.2",
    "autoprefixer": "10.4.20",
    "better-sqlite3": "11.7.0",
    "dotenv": "16.4.7",
    "drizzle-kit": "0.30.1",
    "eslint": "9.17.0",
    "eslint-config-next": "15.1.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-next-on-pages": "1.13.7",
    "postcss": "8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "3.4.17",
    "typescript": "5.7.2",
    "vercel": "39.2.2",
    "wrangler": "3.99.0"
  }
}
