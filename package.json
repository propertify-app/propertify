{
  "name": "propertify",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "npx --legacy-peer-deps @cloudflare/next-on-pages",
    "preview": "npm run pages:build && wrangler pages dev",
    "deploy": "npm run pages:build && wrangler pages deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "db:init": "wrangler d1 execute prod-propertify --command \"SELECT 1;\" --local --yes > /dev/null 2>&1",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "db:studio:local": "npm run db:init && CLOUDFLARE_D1_LOCAL_URL=$(find ./.wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit) npm run db:studio",
    "db:push:local": "npm run db:init && CLOUDFLARE_D1_LOCAL_URL=$(find ./.wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit) npm run db:push",
    "db:migrate:local": "npm run db:init && CLOUDFLARE_D1_LOCAL_URL=$(find ./.wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit) npm run db:migrate"
  },
  "dependencies": {
    "@clerk/elements": "0.19.0",
    "@clerk/nextjs": "6.3.0",
    "@hookform/resolvers": "3.9.1",
    "@radix-ui/react-avatar": "1.1.1",
    "@radix-ui/react-dialog": "1.1.2",
    "@radix-ui/react-dropdown-menu": "2.1.2",
    "@radix-ui/react-label": "2.1.0",
    "@radix-ui/react-separator": "1.1.0",
    "@radix-ui/react-slot": "1.1.0",
    "@radix-ui/react-tooltip": "1.1.3",
    "@tanstack/react-query": "5.59.20",
    "@tanstack/react-query-devtools": "5.59.20",
    "@trpc/client": "11.0.0-rc.630",
    "@trpc/react-query": "11.0.0-rc.630",
    "@trpc/server": "11.0.0-rc.638",
    "bunshi": "2.1.5",
    "class-variance-authority": "0.7.0",
    "client-only": "^0.0.1",
    "cloudflare-workers-cache": "^0.0.6",
    "clsx": "2.1.1",
    "drizzle-orm": "0.36.1",
    "drizzle-zod": "0.5.1",
    "framer-motion": "12.0.0-alpha.1",
    "immer": "10.1.1",
    "jotai": "2.10.1",
    "jotai-immer": "0.4.1",
    "jotai-tanstack-query": "0.9.0",
    "lucide-react": "0.456.0",
    "next": "15.0.3",
    "next-themes": "0.4.3",
    "react": "19.0.0-rc-fb9a90fa48-20240614",
    "react-dom": "19.0.0-rc-fb9a90fa48-20240614",
    "react-hook-form": "7.53.2",
    "react-hook-form-persist": "^3.0.0",
    "react-resizable-panels": "2.1.6",
    "server-only": "^0.0.1",
    "sonner": "1.7.0",
    "svix": "1.39.0",
    "tailwind-merge": "2.5.4",
    "tailwindcss-animate": "1.0.7",
    "uuid": "11.0.2",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "1.13.5",
    "@cloudflare/workers-types": "4.20241106.0",
    "@tanstack/eslint-plugin-query": "5.60.1",
    "@types/node": "22.9.0",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "autoprefixer": "10.4.20",
    "better-sqlite3": "11.5.0",
    "dotenv": "16.4.5",
    "drizzle-kit": "0.28.0",
    "eslint": "9.14.0",
    "eslint-config-next": "15.0.3",
    "eslint-plugin-next-on-pages": "1.13.5",
    "postcss": "8.4.47",
    "tailwindcss": "3.4.14",
    "typescript": "5.6.3",
    "vercel": "37.14.0",
    "wrangler": "3.86.0"
  }
}
